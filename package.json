  "author": "Damiano Barbati <damiano.barbati@gmail.com> (http://damianobarbati.com/)",
  "scripts": {
    "pm2": "./node_modules/pm2/bin/pm2",
    "webpack": "./node_modules/webpack/bin/webpack.js",
    "jsdoc": "./node_modules/jsdoc/jsdoc.js",
    "bin:schema": "node api bin schema",
    "bin:dataset": "node api bin schema && node api bin dataset",
    "build:docs": "yarn jsdoc -- -c ./config/jsdoc.json && open docs/index.html",
    "stop": "yarn pm2 delete ./config/pm2.config.js && yarn pm2 flush && rm -f logs/*",
    "start:dev": "export NODE_ENV=development && yarn pm2 start ./config/pm2.config.js && yarn pm2 logs",
    "start:prod": "export NODE_ENV=production && yarn pm2 start ./config/pm2.config.js && yarn pm2 logs",
    "build:dev": "export NODE_ENV=development && yarn webpack -- --config ./config/webpack.config.js",
    "build:prod": "export NODE_ENV=production && yarn webpack -- --config ./config/webpack.config.js"
  },
  "keywords": [
    "mvp",
    "starter kit",
    "react",
    "koa",
    "es7"
  ],
